.btn {
    width: 50px;
    height: 50px;
    border: 1px solid #fff;
    border-radius: 50%;
}
.left-btn {
    margin-right: 50px;
}
.arrow--left {
    fill: #fff;
    width: 11px;
    height: 11px;
    transform: rotate(180deg);
}

.arrow--right {
    fill: #fff;
    width: 11px;
    height: 11px;
}
